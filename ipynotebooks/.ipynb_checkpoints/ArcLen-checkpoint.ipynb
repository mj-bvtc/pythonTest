   "source": [
    "import math\n",
    "import pint\n",
    "\n",
    "u = pint.UnitRegistry()\n",
    "\n",
    "radius = 397 * u.inch\n",
    "angle = 20.3 * u.degree\n",
    "\n",
    "\n",
    "def get_arc_length(r, ang):\n",
    "    return (2*math.pi*r)*(ang/(360*u.degree))\n",
    "\n",
    "arc = get_arc_length(radius,angle).to(u.foot)\n",
    "print(round(arc, 2))"
   ]
