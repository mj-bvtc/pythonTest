    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def clean(x):\n",
    "    return str(x).upper()\n",
    "\n",
    "\n",
    "\n",
    "def get_flags(text):\n",
    "    text = text.upper()\n",
    "    file = r\"C:\\Users\\mkreidler.BVTCCLOUD\\Desktop\\SPREADSHEET STYLE GUIDE -MAB compiled.xlsx\"\n",
    "    df = pd.read_excel(file)\n",
    "    df.description = df.description.apply(clean)\n",
    "    df[\"flag\"] = np.where(df['description'].str.contains(text, case=False, na=False), 1, 0)\n",
    "    df[\"flag2\"] = np.where(df[\"note\"].str.contains(text, case=False, na=False),1,0)\n",
    "    #df[\"flags\"] = df.flag == 1 or df.flag2 == \n",
    "\n",
    "    df = df.loc[df.flag2 == 1, :]\n",
    "    return df\n",
    "\n",
    "get_flags(\"SAMPLE\")"
   ]
